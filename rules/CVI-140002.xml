<?xml version="1.0" encoding="UTF-8"?>

<cobra document="https://github.com/wufeifei/cobra">
    <name value="Print Param"/>
    <language value="java"/>
    <match><![CDATA[(out\.println\s*\(\s*request\.get(Parameter|QueryString)\s*\(\s*\")]]></match>
    <solution>
        输出入参会导致XSS
    </solution>
    <test>
        <case assert="true"><![CDATA[out.println(request.getParameter("test"))]]></case>
    </test>
    <status value="on"/>
    <author name="Feei" email="feei@feei.cn"/>
</cobra>