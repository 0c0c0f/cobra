<?xml version="1.0" encoding="UTF-8"?>

<cobra document="https://github.com/wufeifei/cobra">
    <name value="LOG Sensitive Information"/>
    <match><![CDATA[(log(ger)?\..*?\(.*(([^a-z]ssn[^a-z])|getssn|ssn\(|socialsecurity|taxid|email|e_mail|emailaddress|pass|amount|account|acct|address|phone|phonenumber|zip|postal))]]></match>
    <repair>
        Possibly logging sensitive information.
    </repair>
    <test>
        <case assert="true"><![CDATA[log.debug('email: test')]]></case>
    </test>
    <status value="on"/>
    <author name="Feei" email="feei@feei.cn"/>
</cobra>